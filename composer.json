{
	"name": "kdabrow/seeder-once",
	"description": "Run your laravel seeders only once",
	"type": "package",
	"keywords": [
		"php",
		"laravel",
		"seeder",
		"database"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Karol DÄ…browski",
			"email": "20278897+karoldabro@users.noreply.github.com",
			"homepage": "https://kdabrow.com"
		}
	],
	"require": {
		"php": ">=8.0.2",
		"illuminate/console": "^10.0",
		"illuminate/database": "^10.0",
		"illuminate/support": "^10.0"
	},
	"require-dev": {
		"orchestra/testbench": "^8.0"
	},
	"autoload": {
		"psr-4": {
			"Kdabrow\\SeederOnce\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Kdabrow\\SeederOnce\\Tests\\": "tests"
		}
	},
	"prefer-stable": true,
	"extra": {
		"laravel": {
			"providers": [
				"Kdabrow\\SeederOnce\\Providers\\SeederOnceProvider"
			]
		}
	}
}
